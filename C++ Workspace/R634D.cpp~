#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef pair<int, int> pii;

int main(){

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    int t;
    cin >> t;
    while(t--){
        int mat[10][10];
        for(int i = 0; i < 9; i++){
            string s;
            cin >> s;
            for(int j = 0; j < 9; j++){
                mat[i][j] = s[j] - 48;
            }
        }
        int center = mat[4][4];
        mat[1][1] = center;
        mat[1][4] = center;
        mat[1][7] = center;
        mat[4][1] = center;
        mat[4][7] = center;
        mat[7][1] = center;
        mat[7][4] = center;
        mat[7][7] = center;
        for(int i = 0; i <9 ;i ++){
            for(int j = 0; j < 9; j++){
                cout << mat[i][j];
            }
            cout << "\n";
        }
        

    }


}

